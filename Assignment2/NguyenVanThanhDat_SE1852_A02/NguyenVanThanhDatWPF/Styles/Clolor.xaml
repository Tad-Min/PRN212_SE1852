<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <!-- Background color -->
    <Color x:Key="BgColor1">#1FC9FD</Color>
    <Color x:Key="BgColor2">#FC0061</Color>
    <Color x:Key="BgColor3">#2E1F3F</Color>
    <Color x:Key="BgColor4">#1F3A60</Color>
    <Color x:Key="BgColor5">#3E3A50</Color>
    <Color x:Key="BgColor6">#4B2E2B</Color>
    <Color x:Key="BgColor7">#1E2A3B</Color>
    

    <!-- Font Color -->
    <SolidColorBrush x:Key="color1">White</SolidColorBrush>
    <SolidColorBrush x:Key="color2">#FFD700</SolidColorBrush>
    <SolidColorBrush x:Key="color3">#90F1EF</SolidColorBrush>
    <SolidColorBrush x:Key="color4">#00FFB3</SolidColorBrush>
    <SolidColorBrush x:Key="color5">#F8A5C2</SolidColorBrush>
    <SolidColorBrush x:Key="color6">#E6F14A</SolidColorBrush>
    <SolidColorBrush x:Key="color7">Black</SolidColorBrush>
    <SolidColorBrush x:Key="color8">#E0E0E0</SolidColorBrush>
    <SolidColorBrush x:Key="color9">#E0F8FF</SolidColorBrush>
    <SolidColorBrush x:Key="color10">#F5E0FF</SolidColorBrush>
    <!-- Border Background Color -->
    <Color x:Key="bdBgColor1">#70D6FF</Color>
    <Color x:Key="bdBgColor2">#C084F4</Color>
    <Color x:Key="bdBgColor3">#FFA07A</Color>
    <Color x:Key="bdBgColor4">#FFE066</Color>
    <Color x:Key="bdBgColor5">#B2F7EF</Color>
    <Color x:Key="bdBgColor6">#FF5DA2</Color>
    <Color x:Key="bdBgColor7">#6C5B7B</Color>
    
    <!-- Button Color -->
    <SolidColorBrush x:Key="btnColor1">#FF2A85</SolidColorBrush>
    <SolidColorBrush x:Key="btnColor2">#0098D0</SolidColorBrush>
    <SolidColorBrush x:Key="btnColor3">#9A60C7</SolidColorBrush>
    <SolidColorBrush x:Key="btnColor4">#FF7A6A</SolidColorBrush>
    <SolidColorBrush x:Key="btnColor5">#36D1DC</SolidColorBrush>
    <!-- Button Border -->
    <Color x:Key="btnBorderColor1">#D47A9A</Color>
    <Color x:Key="btnBorderColor2">#7ED6F9</Color>
    <Color x:Key="btnBorderColor3">#C9A4F4</Color>
    <Color x:Key="btnBorderColor4">#9FC6B9</Color>
    <Color x:Key="btnBorderColor5">#FFE7A7</Color>
</ResourceDictionary>